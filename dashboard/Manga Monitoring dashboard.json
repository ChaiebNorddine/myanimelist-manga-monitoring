        "codeData": "{\n  \"selectedManga\": \"\"\n}",
        "css": "/* Wrapper set to full width with no line breaks */\n.wrapper {\n  overflow-x: auto; /* Enables horizontal scrolling on small screens */\n}\n\n#manga-container {\n  display: flex;\n  flex-wrap: nowrap; /* ‚ùå Prevents wrapping of items to the next line */\n  gap: 1rem;\n  justify-content: center;\n  padding: 10px;\n}\n\n/* Reduced card size to fit 10 side by side */\n.card {\n  flex: 0 0 160px; /* ‚úÖ Fixed width, prevents growing */\n  height: 330px;\n  border: solid #555 2px;\n  border-radius: 10px;\n  padding: 10px;\n  text-align: center;\n  background-color: #1e1e1e;\n  color: white;\n  font-family: Open Sans;\n  display: flex;\n  flex-direction: column;\n  justify-content: space-between;\n  box-sizing: border-box;\n  overflow: hidden;\n  transition: border-color 0.4s ease, transform 0.4s;\n}\n\n.card h3 {\n  font-size: 14px;\n  font-weight: bold;\n  margin: 5px 0;\n  height: 40px;\n  overflow: hidden;\n  text-overflow: ellipsis;\n  display: -webkit-box;\n  -webkit-line-clamp: 2;\n  -webkit-box-orient: vertical;\n  line-height: 1.2;\n  word-break: break-word;\n  cursor: help;\n}\n\n.card img {\n  width: 100px;\n  height: 140px;\n  object-fit: cover;\n  border-radius: 5px;\n  margin: 10px auto;\n}\n\n.card p {\n  font-size: 13px;\n  margin: 4px 0;\n}\n\n/* Animation for recently updated cards */\n.card.updated {\n  animation: flash 0.6s ease;\n}\n\n@keyframes flash {\n  0%   { transform: scale(1.03); }\n  100% { transform: scale(1); }\n}\n\n/* Highlighting top 3 ranked cards */\n.card.rank-1 {\n  border-color: gold;\n}\n.card.rank-2 {\n  border-color: silver;\n}\n.card.rank-3 {\n  border-color: #cd7f32; /* Bronze */\n}\n",
        "dynamicData": false,
        "dynamicFieldDisplayValues": false,
        "dynamicHtmlGraphics": false,
        "dynamicProps": false,
        "html": "<div class=\"wrapper\">\n  <div id=\"manga-container\"></div>\n</div>\n",
        "onInit": "",
        "onInitOnResize": false,
        "onRender": "const container = htmlNode.querySelector('#manga-container'); // ‚úÖ THIS IS THE FIX\ncontainer.innerHTML = \"\";\n\n\n// Helper\nconst getLast = (f) => f.values.get(f.values.length - 1);\n\n// Get series\nconst scoreRankFields = data.series[0]?.fields ?? [];\nconst coverTitleFields = data.series[1]?.fields ?? [];\n\nconst mangaMap = {};\n\n// Extract score + rank\nfor (let i = 1; i < scoreRankFields.length; i++) {\n  const field = scoreRankFields[i];\n  const match = field.name.match(/Manga \"(.*?)\" (Mean Score|Rank)/);\n  if (!match) continue;\n\n  const [ , title, type ] = match;\n  mangaMap[title] = mangaMap[title] || {};\n  mangaMap[title][type === \"Mean Score\" ? \"score\" : \"rank\"] = getLast(field);\n}\n\n// Extract cover + title\nfor (let i = 1; i < coverTitleFields.length; i++) {\n  const field = coverTitleFields[i];\n  const match = field.name.match(/Manga \"(.*?)\" (Cover Image \\(Medium\\)|Title)/);\n  if (!match) continue;\n\n  const [ , title, type ] = match;\n  mangaMap[title] = mangaMap[title] || {};\n  if (type.includes(\"Cover\")) {\n    mangaMap[title].cover = getLast(field);\n  } else {\n    mangaMap[title].title = getLast(field);\n  }\n}\n\n// Build cards\nconst cards = Object.entries(mangaMap)\n  .filter(([_, d]) => d.score && d.rank && d.cover)\n  .map(([title, d]) => {\n    const card = document.createElement(\"div\");\n    card.className = \"card\";\n    card.dataset.rank = d.rank;\n\n    const rankNum = parseInt(d.rank);\n    if (rankNum === 1) card.classList.add(\"rank-1\");\n    else if (rankNum === 2) card.classList.add(\"rank-2\");\n    else if (rankNum === 3) card.classList.add(\"rank-3\");\n\n    card.innerHTML = `\n      <h3 title=\"${title}\">${title}</h3>\n      <img src=\"${d.cover}\" alt=\"cover\" />\n      <p>‚≠ê Score: ${d.score}</p>\n      <p>üèÜ Rank: ${d.rank}</p>\n    `;\n\n    card.classList.add(\"updated\");\n    setTimeout(() => card.classList.remove(\"updated\"), 600);\n\n    return card;\n  })\n  .sort((a, b) => parseFloat(a.dataset.rank) - parseFloat(b.dataset.rank));\n\n// Append to DOM\ncards.forEach(card => container.appendChild(card));\n",
